<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.*?>
<?import java.lang.String?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
            minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" stylesheets="@base.css"
            xmlns="http://javafx.com/javafx/8" fx:controller="app.view.FileController">
    <children>
        <SplitPane dividerPositions="0.5" prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0"
                   AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <items>
                <Pane fx:id="encryptPane" onDragDetected="#dragDetectedE" onDragDone="#dragDoneE"
                      onDragDropped="#dragDroppedE" onDragOver="#dragOver" prefHeight="200.0" prefWidth="200.0">
                    <styleClass>
                        <String fx:value="interactive-pane"/>
                        <String fx:value="interactive-pane-enable"/>
                    </styleClass>
                </Pane>
                <Pane fx:id="decryptPane" onDragDetected="#dragDetectedD" onDragDone="#dragDoneD"
                      onDragDropped="#dragDroppedD" onDragOver="#dragOver" prefHeight="200.0" prefWidth="200.0">
                    <styleClass>
                        <String fx:value="interactive-pane"/>
                        <String fx:value="interactive-pane-enable"/>
                    </styleClass>
                </Pane>
            </items>
        </SplitPane>
    </children>
</AnchorPane>
