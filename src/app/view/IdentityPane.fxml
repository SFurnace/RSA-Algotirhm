<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.Pane?>
<?import java.lang.String?>
<SplitPane xmlns:fx="http://javafx.com/fxml/1" dividerPositions="0.5" maxHeight="-Infinity" maxWidth="-Infinity"
           minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" stylesheets="@base.css"
           xmlns="http://javafx.com/javafx/8" fx:controller="app.view.IdentityController">
    <items>
        <Pane fx:id="identityFilePane" onDragDetected="#identityFilePaneDragDetected"
              onDragDone="#identityFilePaneDragDone" prefHeight="200.0" prefWidth="200.0">
            <styleClass>
                <String fx:value="dropPane"/>
                <String fx:value="dropPane-has-file"/>
            </styleClass>
        </Pane>
        <Pane fx:id="verifyPane" onDragDropped="#verifyPaneDragDropped" onDragOver="#verifyPaneDragOver"
              prefHeight="200.0" prefWidth="200.0">
            <styleClass>
                <String fx:value="dropPane"/>
                <String fx:value="dropPane-can-drop"/>
            </styleClass>
        </Pane>
    </items>
</SplitPane>
