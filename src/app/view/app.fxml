        <Button fx:id="appFileBtn" contentDisplay="GRAPHIC_ONLY" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" styleClass="app-btn" text="File" GridPane.rowIndex="3" />
