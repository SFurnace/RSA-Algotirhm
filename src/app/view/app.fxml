            <Insets />
         </GridPane.margin></Button>
        <Button fx:id="appTextBtn" contentDisplay="GRAPHIC_ONLY" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" styleClass="app-btn" text="Text" GridPane.columnIndex="1" />
