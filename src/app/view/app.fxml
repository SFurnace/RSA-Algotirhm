<GridPane fx:id="appRootPane" prefHeight="400.0" prefWidth="400.0" stylesheets="@app.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.view.AppController">
    <children>
        <Button fx:id="appKeyBtn" contentDisplay="GRAPHIC_ONLY" mnemonicParsing="false" prefHeight="133.0" prefWidth="156.0" styleClass="app-btn" text="Key" GridPane.columnIndex="1" GridPane.rowIndex="2">
